{
  "type": "object",
  "properties": {
    "isAssignedtome": {
      "type": "identifier",
      "selector": "#buglist",
      "control": "exists"
    },
    "issuesList": {
      "type": "array",
      "selectorAll": "tbody tr",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText",
            "selectIndex": 3
          },
          "category": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText",
            "selectIndex": 6
          },
          "status": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText",
            "selectIndex": 8
          },
          "severity": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText",
            "selectIndex": 7
          },
          "goToDetails": {
            "type": "action",
            "selector": "td a[href=\"/mantisbt/view.php?id=\"]",
            "control": "action.link"
          }
        }
      }
    }
  },
  "connect": [
    {
      "match": [
        "isAssignedtome"
      ],
      "action": "issuesList.goToDetails"
    }
  ]
}
