{
  "type": "object",
  "properties": {
    "isAssignedtome": {
      "type": "identifier",
      "selector": "#buglist",
      "control": "exists"
    },
    "issuesList": {
      "type": "array",
      "selectorAll": "tbody tr[bgcolor=\"#c2dfff\"][bgcolor=\"#fcbdbd\"]",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText",
            "selectIndex": 3
          },
          "category": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText",
            "selectIndex": 6
          },
          "status": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText",
            "selectIndex": 8
          }
        }
      }
    }
  },
  "connect": [
    {
      "match": [
        "isAssignedtome"
      ],
      "screens": [
        "viewlist"
      ]
    }
  ]
}
