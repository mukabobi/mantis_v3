{
  "type": "object",
  "properties": {
    "isLanding": {
      "type": "identifier",
      "selector": ".button",
      "control": "exists",
      "selectContains": "Assigned to Me"
    },
    "goToAssignedToMe": {
      "type": "action",
      "selector": "a[href=\"/mantisbt/view_all_bug_page.php\"]",
      "control": "action.link"
    }
  },
  "connect": [
    {
      "match": [
        "isLanding"
      ],
      "action": "goToAssignedToMe"
    }
  ]
}
